{
  "name": "@pinjollist/next-with-analytics",
  "version": "1.0.0-rc.0",
  "description": "Minimalistic analytics helper for Next.js which respects Do Not Track (DNT) headers.",
  "main": "dist/index.js",
  "module": "dist/next-with-analytics.esm.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "author": "Resi Respati <resir014@gmail.com>",
  "homepage": "https://github.com/pinjollist/next-with-analytics",
