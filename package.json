{
  "name": "@pinjollist/next-with-analytics",
  "version": "1.0.1",
  "description": "Minimalistic analytics helper for Next.js which respects Do Not Track (DNT) headers.",
  "main": "dist/index.js",
  "module": "dist/next-with-analytics.esm.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "author": "Resi Respati <resir014@gmail.com>",
  "homepage": "https://github.com/pinjollist/next-with-analytics",
  "bugs": "https://github.com/pinjollist/next-with-analytics/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/pinjollist/next-with-analytics.git"
  },
  "keywords": [
    "nextjs",
    "next",
    "analytics",
    "google-analytics"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdx build",
    "lint": "eslint \"src/**/*\"",
    "prepublishOnly": "tsdx build",
    "report-coverage": "codecov -f ./coverage/*.json",
    "start": "tsdx watch",
    "test": "tsdx test --env=jsdom",
    "type-check": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "peerDependencies": {
    "next": ">= 9.0.0",
    "react": ">= 16.0.0"
  },
  "dependencies": {
    "react-ga": "^3.2.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.1",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/styled-jsx": "^2.2.8",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "codecov": "^3.8.1",
    "eslint": "7.13.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "husky": "^4.3.0",
    "next": "^12.1.0",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "~4.0.5"
  }
}
