    "eslint-plugin-react": "^7.16.0",
    "husky": "^3.1.0",
    "next": "^9.0.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "tsdx": "^0.11.0",
    "tslib": "^1.10.0",
