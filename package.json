  "repository": {
    "type": "git",
    "url": "git://github.com/pinjollist/next-with-analytics.git"
  },
  "keywords": [
    "nextjs",
    "next",
    "analytics",
    "google-analytics"
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
